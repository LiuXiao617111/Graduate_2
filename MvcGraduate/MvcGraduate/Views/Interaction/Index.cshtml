@{
    ViewBag.Title = "InteractionIndex";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section Scripts {
    <script>
        $('.title_navigate a:eq(4) div').addClass('logo_left_title_select');
        $.post(
            "@Url.Action("GetImagesInfo", "Interaction")",
            { "id": 1208203301 },
            function (data) {
                $('.navigation_centre_right').html(data);
            });
        function ManageImage()
        {
            $.post(
                "@Url.Action("GetImagesInfo", "Interaction")",
                { "id": 1208203301 },
                function (data) {
                    $('.navigation_centre_right').html(data);
            });
        }
        function GetArticle()
        {
            $.post(
                "@Url.Action("GetArticle", "Interaction")",
                { "id": 1208203301 },
                function (data) {
                    $('.navigation_centre_right').html(data);
            });
        }
        function GetQuestions() {
            $.post(
                "@Url.Action("GetQuestions", "Interaction")",
                { "id": 1208203301 },
                function (data) {
                    $('.navigation_centre_right').html(data);
            });
        }
        function GetShare_Article() {
            $.post(
                "@Url.Action("GetShareArticle", "Interaction")",
                { "id": 1208203301 },
                function (data) {
                    $('.navigation_centre_right').html(data);
                });
        }
        function GetShare_ImagesInfo() {
            $.post(
                "@Url.Action("GetShareImagesInfo", "Interaction")",
                { "id": 1208203301 },
                function (data) {
                    $('.navigation_centre_right').html(data);
            });
        }
        function GetImages_Comments(id)
        {
            $.post(
                "@Url.Action("GetImagesComments", "Interaction")",
                { "id": id },
                function (data) {
                    $('.navigation_centre_right').html(data);
            });
        }
        function GetShareImages_Comments(id)
        {
            $.post(
                "@Url.Action("GetShareImagesComments", "Interaction")",
                { "id": id },
                function (data) {
                    $('.navigation_centre_right').html(data);
            });
        }
        
    </script>
}

