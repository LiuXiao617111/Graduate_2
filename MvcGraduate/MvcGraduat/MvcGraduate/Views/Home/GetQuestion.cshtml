@model IEnumerable<MvcGraduate.Models.Questions>
<ul class="newtiwen">
    <li class="newtiwen"><a href="#">最新提问</a></li>
    @if (Model != null)
    {
        var i=0;
        foreach (var item in Model)
        {
            if (i++ == 3){
                break;
            }
            <li>
                <a src="#" onclick="window.open('@Url.Action("Details_Question", "Interaction", new { id = item.ID })', '', 'height=500,width=820,status=1,toolbar=no,menubar=no,location=no,scrollbars=yes,left=200,top=100,resizable=yes');">@item.Title</a>
            </li>
        }
    }
</ul>
